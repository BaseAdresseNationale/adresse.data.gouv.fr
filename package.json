  "scripts": {
    "dev": "node server",
    "build-next": "next build",
    "build-available-flags": "node scripts/build-available-flags",
    "build": "yarn build-available-flags && yarn build-next",
    "start": "NODE_ENV=production node server",
    "lint": "xo",
