    "blob-to-buffer": "^1.2.9",
    "chardet": "^1.5.1",
    "chart.js": "^3.9.1",
    "compression": "^1.7.4",
    "content-disposition": "^0.5.4",
    "copy-to-clipboard": "^3.3.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "geojson-vt": "^3.2.1",
    "got": "^11.8.6",
    "helmet": "^4.6.0",
    "hoist-non-react-statics": "^3.3.2",
